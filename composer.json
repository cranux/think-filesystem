{
    "name": "cranux/think-filesystem",
    "description": "The ThinkPHP8.0 Filesystem Package",
    "type": "library",
    "license": "Apache-2.0",

    "autoload": {
        "psr-4": {
            "think\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "think\\tests\\": "tests/"
        }
    },
    "require": {
        "topthink/framework": "^6.1|^8.0",
        "league/flysystem": "^3.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
